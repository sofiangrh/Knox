<!-- Titlebar -->
	<draw_ops name="bg_active">
	  	<rectangle color="shade/gtk:bg[SELECTED]/1.15" x="1" y="1" width="width-3" height="height-1"/>

	  	

		 <line color="shade/gtk:bg[SELECTED]/1.15" width="1" x1="2" y1="1" x2="width-2" y2="1"/>
		 <line color="blend/black/gtk:bg[SELECTED]/0.6" width="1" x1="3" y1="height - 1" x2="width-3" y2="height - 1"/>

	  	<rectangle color="blend/black/gtk:bg[SELECTED]/0.6" filled="true" x="0" y="0" width="1" height="1"/>
	  	<rectangle color="blend/black/gtk:bg[SELECTED]/0.6" filled="true" x="width-1" y="0" width="1" height="1"/>
	</draw_ops>

	<draw_ops name="bg_active-i">
	  	<rectangle color="shade/gtk:bg[NORMAL]/1.02" filled="true" x="2" y="1" width="width-4" height="height/2"/>
		<line color="shade/gtk:bg[NORMAL]/1.0" width="1" x1="2" y1="height/2" x2="width-2" y2="height/2"/>
	  	<rectangle color="shade/gtk:bg[NORMAL]/0.97" filled="true" x="2" y="height/2+1" width="width-4" height="height/2-1"/>

		 <line color="shade/gtk:bg[NORMAL]/1.05" width="1" x1="2" y1="1" x2="width-2" y2="1"/>
		 <line color="blend/black/gtk:bg[NORMAL]/0.7" width="1" x1="3" y1="height - 1" x2="width-3" y2="height - 1"/>

	  	<rectangle color="blend/black/gtk:bg[NORMAL]/0.8" filled="true" x="0" y="0" width="1" height="1"/>
	  	<rectangle color="blend/black/gtk:bg[NORMAL]/0.8" filled="true" x="width-1" y="0" width="1" height="1"/>
	</draw_ops>

	<draw_ops name="bg_active_shaded">
	  	<include name="bg_active"/>
